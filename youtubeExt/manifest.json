  {
    "name": "YOUTUBE blocker",
    "version": "1.0.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/*"
        ],
        "js": [
          "DOMchange.js"
        ],
        "run_at": "document_start"
      }
    ]
  }